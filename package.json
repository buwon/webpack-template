{
  "name": "webpack-template",
  "version": "1.0.0",
  "main": "index.ts",
  "license": "MIT",
  "scripts": {
    "task:clean": "rm -rf dist/",
    "task:build": "webpack --mode='development' --devtool='source-map'",
    "task:dist": "webpack",
    "task:watch": "npm run task:build -- --watch",
    "clean": "run-s task:clean",
    "build": "run-s task:clean task:build",
    "dist": "run-s task:clean task:dist",
    "watch": "run-s task:clean task:watch"
  },
  "dependencies": {
    "lodash": "^4.17.20",
    "prettier": "^2.1.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.161",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.6.2",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "eslint": "^7.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-scope": "^5.1.0",
    "mocha": "^8.1.3",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^2.5.2"
  }
}
