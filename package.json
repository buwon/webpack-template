{
  "name": "webpack-template",
  "version": "1.0.0",
  "main": "index.ts",
  "license": "MIT",
  "scripts": {
    "task:clean": "rm -rf dist/",
    "task:build": "webpack --mode='development' --devtool='source-map'",
    "task:dist": "webpack",
    "task:watch": "npm run task:build -- --watch",
    "clean": "run-s task:clean",
    "build": "run-s task:clean task:build",
    "dist": "run-s task:clean task:dist",
    "watch": "run-s task:clean task:watch"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "prettier": "^2.1.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.165",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.12.1",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-scope": "^5.1.1",
    "mocha": "^8.2.1",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^8.0.9",
    "typescript": "^4.0.5",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "webpack-node-externals": "^2.5.2"
  }
}
